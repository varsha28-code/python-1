class Solution(object):
    def postorderTraversal(self, root):
        """
        :type root: Optional[TreeNode]
        :rtype: List[int]
        """
        result = []
        def dfs(node):
            if not node:
                return
            dfs(node.left)     # Left
            dfs(node.right)    # Right
            result.append(node.val)  # Root
        dfs(root)
        return result
        
