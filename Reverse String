class Solution:
    def reverseString(self, s):
        left, right = 0, len(s) - 1
        while left < right:
            s[left], s[right] = s[right], s[left]  # swap
            left += 1
            right -= 1



class Solution:
    def reverseString(self, s: str) -> str:
        return s[::-1]

