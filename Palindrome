def isPalindrome(self, x):
		numbers = []
        if (x < 0 or x % 10 == 0 and x != 0): ## Edge cases 
            return False
        while x > 0:
            numbers.append(x % 10)
            x = (x - x % 10) // 10
        originalNumbers = numbers[::-1]
        if numbers == originalNumbers:
            return True
        else:
            return False
        
